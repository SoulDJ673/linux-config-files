# UTILITY
alias zshconfig="vim ~/.zshrc"
alias fixhtml="tidy -i -m -w 160 -ashtml -utf8"
alias ls="ls --color=always"
alias neofetch="neofetch | sed -r '/^\s*$/d' | cat - && echo '\n'"

# SCHOOL
alias pushschoolwork="cd ~/Documents/school-work-backup && bash push"
alias gotoschool="cd ~/Documents/school-work-backup"

# GIT
alias ga="git add"
alias gc="git commit -m"

